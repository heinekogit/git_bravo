<%= form_for(@image, :url => {:action => 'create'}) do |f| %>
    <div class="field">
        <p><%= f.label :filename %></p>
        <p><%= f.file_field :filename %></p>
        <p><%= f.label :comment %></p>
        <p><%= f.text_field :comment, size: "60x12" %></p>
      <%= f.hidden_field :route_id %>
    </div>
    <div class="actions" %>
<%= f.submit "書き込む" %>
    </div>
<% end %>